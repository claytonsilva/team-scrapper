{
  "name": "team-scrapper",
  "version": "0.1.0",
  "author": {
    "email": "clayton.deploy@gmail.com",
    "name": "Clayton Santos da Silva",
    "url": "https://medium.com/@claytonssilva"
  },
  "private": false,
  "license": "BSD-3-Clause",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src -d dist --source-maps",
    "watch": "babel src -w -d dist --source-maps",
    "depcheck": "depcheck",
    "test-ci": "ENV_NAME=test jest --ci --reporters=jest-junit --outputFile=./test-results/jest",
    "start": "node dist/ports/http/bin/www",
    "test": "ENV_NAME=test jest",
    "lint": "eslint -c .eslintrc src/**/*.js",
    "stryker-mutate": "stryker run",
    "doc": "jsdoc ./src/**/*.js  -d ./docs -c ./jsdoc.conf.json"
  },
  "jest": {
    "verbose": true
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "./test-results/jest",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "engines": {
    "node": "^12.18.1",
    "yarn": "^1.20.0",
    "npm": "^6.0.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "axios": "^0.20.0",
    "debug": "^4.1.1",
    "escriba": "^2.5.0",
    "log4js": "^5.1.0",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.2.3",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@stryker-mutator/core": "^3.2.4",
    "@stryker-mutator/javascript-mutator": "^3.2.4",
    "@stryker-mutator/jest-runner": "^3.2.4",
    "@types/jest": "^25.1.4",
    "@types/ramda": "0.26.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.1.0",
    "commitlint": "^7.2.1",
    "depcheck": "^0.9.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^1.3.1",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "jsdoc": "^3.6.3",
    "lint-staged": "^8.1.0",
    "prettier-eslint": "8.8.2",
    "rimraf": "^3.0.0",
    "standard": "^14.3.1"
  },
  "resolutions": {
    "dd-trace": "^0.14.0",
    "typescript": ">=2.8.0"
  }
}
